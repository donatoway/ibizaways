{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/animations/Typewriters.js", "src/pages/SunsetTrip/sunsetList.js", "src/pages/Yatchs/Yatch-detail/index.js", "src/YatchDescriptions", "src/components/Carousel/carousel.js", "src/components/Carousel/carousel.js", "src/components/Carousel/carousel.js", "src/pages/Yatchs/Yatchs-index/index.js", "src/components/Images-Carousel/images-carousel.js"],
  "exclude": ["node_modules"]
}
